#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
  string a,b;
  cin>>a>>b;
  int len=a.size();
  int len1=b.size();
  cout<<len<<" "<<len1<<"\n";
  cout<<a+b<<"\n";
  
  char c0,c1;
  c0=a[0];
  c1=b[0];
  a[0]=c1;
  b[0]=c0;
 
  cout<<a<<" "<<b;
    return 0;
}
